{
  "name": "markdown-servicenow",
  "version": "1.0.1",
  "description": "Convert Markdown to ServiceNow journal field format with rich HTML formatting",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "markdown",
    "servicenow",
    "converter",
    "journal",
    "html",
    "formatter"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Prakhar Yadav",
  "repository": {
    "type": "git",
    "url": "https://github.com/yadav-prakhar/markdown-servicenow"
  }
}
